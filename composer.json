{
  "name": "touralis/mis-fichajes",
  "description": "Sistema de fichaje de empleados para Laravel",
  "type": "library",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Tu Nombre",
      "email": "tu@empresa.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/console": "^10.0|^11.0",
    "illuminate/filesystem": "^10.0|^11.0",
    "livewire/livewire": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Touralis\\MisFichajes\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Touralis\\MisFichajes\\MisFichajesServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
